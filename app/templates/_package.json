{
  "name": "<%= app %>",
  "version": "0.0.1",
  "main": "src/<%= app %>.js",
  "scripts":
  {
    "test-client": "gulp coverage",
    "test-server": "mocha ./tests/*_test.js --recursive --check-leaks --reporter min",
    "coverage-server": "istanbul cover ./node_modules/mocha/bin/_mocha -- tests/*_test.js",
    "coveralls-server": "istanbul cover ./node_modules/mocha/bin/_mocha -- tests/*_test.js --report lcovonly && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies":
  {
    "lodash": "^3.0.0",
    "bluebird": "^2.3.11",
    "body-parser": "^1.10.1",
    "express": "~4.5.1",
    "express-content-length-validator": "0.0.4",
    "mongoose": "^3.8.23",
    "morgan": "^1.1.1",
    "socket.io": "^1.2.1",
    "socket.io-client": "^1.3.2",
    "newrelic": "^1.14.0"

  },
  "devDependencies":
  {
    "del": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-rev": "^3.0.0",
    "gulp-usemin": "^0.3.11",
    "karma-ng-html2js-preprocessor": "^0.1.2",
    "gulp-coveralls": "^0.1.3",
    "gulp-rename": "^1.2.0",
    "gulp-uglify": "^1.1.0",
    "jasmine-core": "^2.1.3",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.3",
    "karma-ie-launcher": "^0.1.5",
    "karma-jasmine": "^0.3.3",
    "karma-opera-launcher": "^0.1.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "mocha": "^2.1.0",
    "chai": "^2.0.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5"
  },
  "repository":
  {
    "type": "git",
    "url": "https://github.com/<%= username %>/<%= app %>.git"
  },
  "author": "<%= username %>",
  "license": "MIT",
  "bugs":
  {
    "url": "https://github.com/<%= username %>/<%= app %>/issues"
  },
  "homepage": "https://github.com/<%= username %>/<%= app %>",
  "engines":
  {
    "iojs": "1.x.x"
  }
}
