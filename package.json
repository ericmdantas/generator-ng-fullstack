  "description": "Next generation stack. Use the best latest stuff: Node, Go, http/2, Angular 1, Angular 2, Express, MongoDB, Gulp, Babel, Typescript and much more.",
  "license": "MIT",
  "main": "app/index.js",
  "repository": "ericmdantas/generator-ng-fullstack",
  "author": {
    "name": "Eric Mendes Dantas",
    "email": "ericdantas0@gmail.com",
    "url": "https://github.com/ericmdantas"
  },
  "engines": {
    "node": "stable"
  },
  "scripts": {
    "prepublish": "npm t",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/  --report lcovonly -- --recursive -R spec --check-leaks --require babel-core/register && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "mocha test/ --recursive --reporter list --check-leaks --require babel-core/register"
  },
  "keywords": [
    "yeoman-generator",
    "angular-generator",
    "next generation",
    "ng-fullstack",
    "generator-angular-fullstack",
    "generator-ng-fullstack",
    "generator-iojs-fullstack",
    "angular",
    "angularjs",
    "angular2",
    "generator-es6",
    "go",
    "http2",
    "go-http2",
    "ng2",
    "go-generator",
    "generator-go",
    "generator-ng2",
    "generator-go-fullstack"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "lodash": "^3.10.1",
    "yeoman-generator": "^0.19.0",
    "yosay": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.3.0",
    "coveralls": "^2.11.8",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "istanbul": "^0.4.2",
    "mocha": "*",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
